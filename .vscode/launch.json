{
  "configurations": [
    {
      "args": [
        "--extensionDevelopmentPath=${workspaceFolder}"
      ],
      "env": {
        "NODE_ENV": "debug"
      },
      "name": "Run Extension",
      "outFiles": [
        "${workspaceFolder}/extension.js"
      ],
      "request": "launch",
      "runtimeExecutable": "${execPath}",
      "type": "extensionHost"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Node App",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/extension.js",
      "args": [
        "tests/test-presteps.yml"
      ]
    }
  ],
  "version": "0.2.0"
}
